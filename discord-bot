from http import server
import discord
import os

class MyClient(discord.Client):
    
    
    async def on_ready(self):
        
        print('Logged on as {0}!'.format(self.user))
        

    async def on_message(self, message):
        
        print('Message from {0.author}: {0.content}'.format(message))
        if message.content == '!verdades' or message.content == '!Joao':

            await message.channel.send(f'1-  João é o mais pika {os.linesep}2 - Dada é viado{os.linesep}3 - mauricio é baiter{os.linesep}4 - felipin é pessimo{os.linesep}5 - hamu é tiltado{os.linesep}6 - tata é do direito logo ruim{os.linesep}7 - venzi é chato{os.linesep}8 - jonh é mais chato ainda{os.linesep}9 - sorriso só joga de awp{os.linesep}10 - amaral so tem 1 pró : eagle lama.{os.linesep}11 - moon é mulher{os.linesep}12 - tcholas.')
        if message.content == '!celo':
            
            await message.channel.send(f'Celo já rushou e entregou {round} rounds. Parabéns fracassado')
        if message.content == '!hamu':
            await message.channel.send('Hamú ja deixou x  maes fudidas ao acordar!')   
client = MyClient()

